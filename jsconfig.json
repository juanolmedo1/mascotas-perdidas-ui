{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@app/*": ["./app/*"],
      "@assets/*": ["app/assets/*"],
      "@locales/*": ["app/locales/*"],
      "@store/*": ["app/store/*"],
      "@styles/*": ["app/styles/*"],
      "@core/*": ["app/modules/core/*"],
      "@home/*": ["app/modules/home/*"],
      "@login/*": ["app/modules/login/*"]
    }
  },
  "exclude": ["node_modules", "android", "ios"]
}
